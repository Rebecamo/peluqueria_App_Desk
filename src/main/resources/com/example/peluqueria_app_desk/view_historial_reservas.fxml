<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="517.0" prefWidth="716.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.peluqueria_app_desk.HistorialReservasController">
   <children>
      <MenuBar fx:id="menuBar" layoutX="-1.0" layoutY="2.0" prefHeight="36.0" prefWidth="723.0">
        <menus>
          <Menu mnemonicParsing="false" text="Inicio">
            <items>
              <MenuItem fx:id="menuItemInicio" mnemonicParsing="false" text="Inicio de sesión" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Gestionar horarios">
            <items>
              <MenuItem fx:id="menuItemGestionHorario" mnemonicParsing="false" text="Gestionar" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Gestionar reservas">
            <items>
              <MenuItem fx:id="menuItemGestionReservas" mnemonicParsing="false" text="Gestionar" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Gestión">
              <items>
                <MenuItem fx:id="menuItemAtras" mnemonicParsing="false" text="Ir a gestión" />
              </items>
            </Menu>
        </menus>
      </MenuBar>
      <TableView fx:id="tblHistorial" layoutX="10.0" layoutY="59.0" prefHeight="200.0" prefWidth="697.0">
        <columns>
          <TableColumn fx:id="clidHistorial" prefWidth="75.0" text="ID Historial" />
          <TableColumn fx:id="clidReserva" prefWidth="75.0" text="ID Reserva" />
            <TableColumn fx:id="clCliente" prefWidth="75.0" text="Cliente" />
            <TableColumn fx:id="clEmpleado" prefWidth="75.0" text="Empleado" />
            <TableColumn fx:id="clServicio" prefWidth="75.0" text="Servicio" />
            <TableColumn fx:id="clFechaReserva" prefWidth="75.0" text="Fecha" />
            <TableColumn fx:id="clHoraReserva" prefWidth="75.0" text="Hora" />
            <TableColumn fx:id="clEstadoReserva" prefWidth="75.0" text="Estado " />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
